{
  "expo": {
    "name": "Helper",
    "slug": "helper",
    "description": "Helper - application for people",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#16171D",
    "primaryColor": "#6C38CC",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#16171D"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.kostya.zhirnov.helper"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/FiraSans-Regular.ttf",
            "./assets/fonts/FiraSans-SemiBold.ttf"
          ]
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#16171D"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Permission is required to select a profile photo."
        }
      ]
    ],
    "scheme": "helper",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "504d71d9-b9e8-4fd1-81a9-24062e502f42"
      }
    },
    "owner": "kostya.zhirnov"
  }
}
